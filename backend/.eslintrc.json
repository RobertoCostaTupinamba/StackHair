{
  "env": {
      "commonjs": true,
      "es6": true,
      "node": true
  },
  "extends": [
      "airbnb-base",
      "plugin:prettier/recommended"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    // "radix":"off",
    "no-plusplus":"off",
    "no-loop-func": "off",
    "no-console":"off",
    "func-names":"off",
    "no-await-in-loop":"off",
    "no-unused-vars":"off",
    "no-restricted-syntax": "off",
    "no-underscore-dangle":"off",
    "consistent-return":"off",
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never"
      }
    ]
  }
}
